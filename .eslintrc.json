{
  "extends": "fullstack",
  "root": true,
  "rules": {
    "semi": [0, "always"],
    "no-unused-vars": 0,
    "react/no-unused-state": 0,
    "quotes": "off",
    "react/jsx-key": "off",
    "react/button-has-type": "off",
    "react/no-access-state-in-setstate": "off",
    "no-nested-ternary": "off",
    "key-spacing": "off"
  }
}
